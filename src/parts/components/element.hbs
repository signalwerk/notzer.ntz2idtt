{{#ifCond processor.type 'text'}}
  {{#if value}}
    {{text value}}
  {{/if}}
  {{#if processor.content-after}}
    {{fixture processor.content-after}}
  {{/if}}
{{/ifCond}}

{{#ifCond processor.type 'paragraph'}}
  {{#ParagraphStyle processor.title}}
    {{> all children}}

    {{#if processor.content-after}}
      {{fixture processor.content-after}}
    {{/if}}
  {{/ParagraphStyle}}
{{/ifCond}}

{{#ifCond processor.type 'inline'}}
  {{#CharStyle processor.title}}
    {{> all children}}

    {{#if processor.content-after}}
      {{fixture processor.content-after}}
    {{/if}}
  {{/CharStyle}}
{{/ifCond}}

{{#ifCond processor.type 'table'}}
  {{>table}}
{{/ifCond}}

{{#ifCond processor.type 'tr'}}
  {{> all children}}
{{/ifCond}}

{{#ifCond processor.type 'td'}}
  {{> all children}}
{{/ifCond}}

{{#ifCond processor.type 'th'}}
  {{> all children}}
{{/ifCond}}

{{#ifCond processor.type 'root'}}
  {{> all children}}
{{/ifCond}}
