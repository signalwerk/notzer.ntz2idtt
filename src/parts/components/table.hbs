{{#TableStyle processor.title}}
  {{#Table processor.rowCount processor.columnCount}}
    {{#each processor.colWidth}}
      {{ColWidth this}}
    {{/each}}
    {{#each processor.rows}}
      {{#Row style.min-height}}
        {{#each children}}
          {{#CellStyle processor.title}}
            {{#Cell span}}
              {{> all children}}
            {{/Cell}}
          {{/CellStyle}}
        {{/each}}
      {{/Row}}
		{{/each}}
  {{/Table}}
{{/TableStyle}}
